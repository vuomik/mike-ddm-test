{
  "type": "module",
  "scripts": {
    "build:client": "npx vite build --ssrManifest --outDir dist/client",
    "build:server": "npx vite build --ssr server/server.ts --outDir dist/server",
    "build:ssr": "npx vite build --ssr src/entry-server.ts --outDir dist/ssr",
    "build": "npm run build:client && npm run build:server && npm run build:ssr",
    "dev": "NODE_ENV=development tsx server/server.ts",
    "start": "NODE_ENV=production node dist/server/server.js",
    "lint": "eslint . --ext .js,.ts,.vue",
    "lint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "dependencies": {
    "@types/uuid": "^10.0.0",
    "axios": "^1.11.0",
    "dotenv": "^16.6.0",
    "express": "^4.18.2",
    "express-async-handler": "^1.2.0",
    "http-errors": "^2.0.0",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "vue": "^3.5.17",
    "vue-router": "^4.5.1",
    "xml2js": "^0.6.2",
    "zod": "^4.0.10"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.11",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.4",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.32.0",
    "eslint-config-love": "^121.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-promise": "^6.6.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "vite": "^7.0.0",
    "vue-tsc": "^2.2.10"
  }
